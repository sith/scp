<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


    <bean id="jaxb2Marshaller" class="org.springframework.oxm.jaxb.Jaxb2Marshaller">
        <property name="contextPath" value="org.tnt.scp.common.generated"/>
        <property name="schemas">
            <list>
                <value>org/tnt/scp/uiservices/xsd/script.xsd</value>
                <value>org/tnt/scp/uiservices/xsd/scene.xsd</value>
            </list>
        </property>
    </bean>

    <bean id="contextHandler" class="org.tnt.scp.uiservices.installer.ContextHandler"/>

</beans>